                    </elementSpec>


                    <!-- add new attributes "topLine" and "rulingMedium" to layout -->
                    <elementSpec ident="layout" module="msdescription" mode="change">
                        <attList>
                            <attDef ident="topLine" mode="add">
                                <desc>Specifies the whether the writing is above top line, below top
                                    line, or mixed. Note: This is a customization which is not currently part of the TEI P5 standard.</desc>
                                <valList type="closed">
                                    <valItem ident="above">
                                        <desc>The writing is above top line.</desc>
                                    </valItem>
                                    <valItem ident="below">
                                        <desc>The writing is below top line.</desc>
                                    </valItem>
                                    <valItem ident="mixed">
                                        <desc>The writing is variously above and below top line with
                                            no clear pattern.</desc>
                                    </valItem>

                                </valList>
                            </attDef>
                            <attDef ident="rulingMedium" mode="add">
                                <desc>Specifies the medium <!-- OK? --> used to carry out the
                                    ruling. Note: This is a customization which is not currently part of the TEI P5 standard.</desc>
                                <datatype minOccurs="1" maxOccurs="1">
                                    <dataRef key="teidata.enumerated"/>
                                </datatype>
                                <valList type="semi">
                                    <valItem ident="ink">
                                        <desc>The ruling is carried out using ink.</desc>
                                    </valItem>
                                    <valItem ident="leadpoint">
                                        <desc>The ruling is carried out using leadpoint or
                                            plummet.</desc>
                                    </valItem>
                                    <valItem ident="hardpoint">
                                        <desc>The ruling is carried out using hardpoint or
                                            drypoint.</desc>
                                    </valItem>
                                    <valItem ident="crayon">
                                        <desc>The ruling is carried out using ‘crayon’.</desc>
                                    </valItem>
                                    <valItem ident="board">
                                        <desc>The ruling is carried out using a board.</desc>
                                    </valItem>

                                </valList>
                            </attDef>
                        </attList>
                    </elementSpec>
                    
                    <!-- add new attribute value "offsite" for the availability element -->
                    <elementSpec ident="availability" module="header" mode="change">
                        <attList>
                            <attDef ident="status" mode="replace">
                                <desc>Specifies the availability of a manuscript. This attribute should not be used to specify the availability of manuscript metadata: use the <gi>licence</gi> element for that purpose.</desc>
                                <datatype maxOccurs="unbounded">
                                    <dataRef key="teidata.word"/>
                                </datatype>
                                <valList type="closed">
                                    <valItem ident="free">
                                        <desc>The item is available.</desc>
                                    </valItem>
                                    <valItem ident="restricted">
                                        <desc>Access to the item is permanently restricted for conservation or other reasons.</desc>
                                    </valItem>
                                    <valItem ident="exhibition">
                                        <desc>The item is on exhibition at the owning library or another institution, and is likely to be temporarily unavailable.</desc>
                                    </valItem>
                                    <valItem ident="offsite">
                                        <desc>The item is stored offsite, and will take at least twenty-four hours to produce.</desc>
                                    </valItem>
                                    <valItem ident="unknown">
                                        <desc>Availability of the item is unknown.</desc>
                                    </valItem>
                                </valList>
                            </attDef>
                        </attList>
                    </elementSpec>
                    
                    
                    
                    

                    <!-- ********************** -->
                    <!-- Schematron constraints -->
                    <!-- ********************** -->


                    <!-- msDesc[1]/@xml:id must match filename -->
                    <elementSpec ident="msDesc" module="msdescription" mode="change">
                        <constraintSpec scheme="schematron" ident="msDesc-xmlid">
                            <constraint>
                                <sch:rule
                                    context="/tei:TEI[not(contains(base-uri(.), 'genizah-mss'))]">
                                    <sch:assert 
                                        test="matches(@xml:id, '^manuscript_\d+$')" 
                                        role="fatal">
                                        The root TEI element must have an @xml:id beginning with
                                        "manuscript_" then a number (which must be unique across the
                                        entire collection).
                                    </sch:assert>
                                </sch:rule>
                                <sch:rule 
                                    context="/tei:TEI[contains(base-uri(.), 'genizah-mss')]">
                                    <sch:assert test="matches(@xml:id, '^volume_\d+$')" 
                                        role="fatal">
                                        The root TEI element must have an @xml:id beginning with "volume_" 
                                        then a number (which must be unique across the entire collection).
                                    </sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                    </elementSpec>


                    <!-- author inside msItem must have @key -->
                    <elementSpec ident="author" module="core" mode="change">
                        <constraintSpec ident="author.key.check" scheme="isoschematron">
                            <constraint>
                                <sch:rule
                                    context="//tei:msItem//tei:author[ancestor::tei:fileDesc[descendant::tei:idno[@type='catalogue']='Western']]">
                                    <sch:assert test="@key[matches(., 'person_\d+')]">In the
                                        medieval catalogue, the author element, when a child of
                                        msItem, must have a key matching the pattern
                                        'person_\d+'.</sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                    </elementSpec>

                    <!-- persName inside msDesc must have a key -->
                    <elementSpec ident="persName" module="namesdates" mode="change">
                        <constraintSpec ident="persName.key.check" scheme="isoschematron">
                            <constraint>
                                <sch:rule
                                    context="//tei:msDesc//tei:persName[ancestor::tei:fileDesc[descendant::tei:idno[@type='catalogue']='Western']]">
                                    <sch:assert test="@key[matches(., 'person_\d+')]">In the
                                        medieval catalogue, the persName element, when a descendant
                                        of msDesc, must have a key matching the pattern
                                        'person_\d+'.</sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                    </elementSpec>

                    <!-- placeName inside msDesc must have a key -->
                    <elementSpec ident="placeName" module="namesdates" mode="change">
                        <constraintSpec ident="placeName.key.check" scheme="isoschematron">
                            <constraint>
                                <sch:rule
                                    context="//tei:msDesc//tei:placeName[ancestor::tei:fileDesc[descendant::tei:idno[@type='catalogue']='Western']]">
                                    <sch:assert test="@key[matches(., 'place_\d+')]">In the medieval
                                        catalogue, the placeName element, when a descendant of
                                        msDesc, must have a key matching the pattern
                                        'place_\d+'.</sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                    </elementSpec>

                    <!-- orgName inside msDesc must have a key -->
                    <elementSpec ident="orgName" module="namesdates" mode="change">
                        <constraintSpec ident="orgName.key.check" scheme="isoschematron">
                            <constraint>
                                <sch:rule
                                    context="//tei:msDesc//tei:orgName[ancestor::tei:fileDesc[descendant::tei:idno[@type='catalogue']='Western']]">
                                    <sch:assert test="@key[matches(., 'org_\d+')]">In the medieval
                                        catalogue, the orgName element, when a descendant of msDesc,
                                        must have a key matching the pattern 'org_\d+'.</sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                    </elementSpec>

                    <!-- not yet adding a rule about @key for works -->

                    <!-- county inside origin must have a key -->
                    <elementSpec ident="country" module="namesdates" mode="change">
                        <constraintSpec ident="country.key.check" scheme="isoschematron">
                            <constraint>
                                <sch:rule
                                    context="//tei:origin//tei:country[ancestor::tei:fileDesc[descendant::tei:idno[@type='catalogue']='Western']]">
                                    <sch:assert test="@key[matches(., 'place_\d+')]">In the medieval
                                        catalogue, the country element, when a descendant of origin,
                                        must have a key matching the pattern
                                        'place_\d+'.</sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                    </elementSpec>

                    <!-- settlement inside origin must have a key -->
                    <elementSpec ident="settlement" module="namesdates" mode="change">
                        <constraintSpec ident="settlement.key.check" scheme="isoschematron">
                            <constraint>
                                <sch:rule
                                    context="//tei:origin//tei:settlement[ancestor::tei:fileDesc[descendant::tei:idno[@type='catalogue']='Western']]">
                                    <sch:assert test="@key[matches(., 'place_\d+')]">In the medieval
                                        catalogue, the settlement element, when a descendant of
                                        origin, must have a key matching the pattern
                                        'place_\d+'.</sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                    </elementSpec>

                    <!-- a manuscript must have one or more languages  -->
                    <elementSpec ident="textLang" module="core" mode="change">
                        <constraintSpec ident="textLang.check" scheme="schematron">
                            <constraint>
                                <sch:rule
                                    context="//tei:msContents">
                                    <sch:assert 
                                        test="descendant::tei:textLang" 
                                        role="warn">
                                        A manuscript should have one or more languages recorded in a textLang element. 
                                        For manuscripts with no linguistic content use code 'zxx'. If the language is 
                                        undetermined use the code 'und'.
                                    </sch:assert>
                                </sch:rule>
                                <sch:rule
                                    context="//tei:textLang">
                                    <sch:assert 
                                        test="@mainLang and string-length(normalize-space(string())) gt 0" 
                                        role="error">
                                        The predominant language must be recorded in using a code in a mainLang attribute 
                                        (and an otherLang attribute if there are other languages) and described as text 
                                        within the textLang element.
                                    </sch:assert>
                                </sch:rule>
                                <sch:rule
                                    context="//tei:textLang/@mainLang | //tei:textLang/@otherLangs | //@xml:lang">
                                    <sch:assert 
                                        test="every $code in tokenize(., ' ') satisfies matches($code, '^[a-z]{2,3}(-|$)')" 
                                        role="error">
                                        Codes in <sch:value-of select="name(.)"/> attributes must conform to BCP 47 
                                        (https://tools.ietf.org/html/bcp47), starting with an ISO 639 code for the language, 
                                        then optionally further codes for the script (ISO 15924), region, transliteration, etc.
                                    </sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                    </elementSpec>

                    <!-- support must be recorded in @support attribute -->
                    <elementSpec ident="supportDesc" module="msdescription" mode="change">
                        <constraintSpec ident="supportDesc.check" scheme="schematron">
                            <constraint>
                                <sch:rule
                                    context="//tei:supportDesc">
                                    <sch:assert 
                                        test="@material" 
                                        role="warn">
                                        The writing support of a manuscript should be specified in the
                                        material attribute on the element supportDesc.
                                    </sch:assert>
                                    <!-- a more detailed rule could be added here, to check correct attribute values -->
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                    </elementSpec>

                    <!-- binding element must have dating attributes -->
                    <elementSpec ident="binding" module="msdescription" mode="change">
                        <constraintSpec ident="binding.check" scheme="schematron">
                            <constraint>
                                <sch:rule
                                    context="//tei:binding">
                                    <sch:assert 
                                        test="@when | @notBefore | @notAfter | @contemporary='true'" 
                                        role="warn">
                                        The binding element should have dating attributes (when or notBefore/notAfter) 
                                        or a contemporary attribute (with the value 'true').
                                    </sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                    </elementSpec>

                    <!-- origDate element must have dating attributes -->
                    <elementSpec ident="origDate" module="msdescription" mode="change">
                        <constraintSpec ident="origDate.check" scheme="schematron">
                            <constraint>
                                <sch:rule
                                    context="//tei:origDate">
                                    <sch:assert 
                                        test="@calendar and (@when | @notBefore | @notAfter | @from | @to) and string-length(normalize-space(string())) gt 0" 
                                        role="error">
                                        The origDate element must have two or more attributes - calendar and at least one of
                                        when, notBefore, notAfter, from and/or to - and must contain some text describing the date.
                                    </sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                    </elementSpec>


                </schemaSpec>




            </div>
        </body>
    </text>

</TEI>
